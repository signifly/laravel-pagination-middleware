{
    "name": "signifly/laravel-pagination-middleware",
    "description": "A pagination middleware for your Laravel app.",
    "homepage": "https://github.com/signifly/laravel-pagination-middleware",
    "license": "MIT",
    "authors": [
        {
            "name": "Morten Poul Jensen",
            "email": "mpj@signifly.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1|^7.0",
        "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0"
    },
    "autoload": {
        "psr-4": {
            "Signifly\\Pagination\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Signifly\\Pagination\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Signifly\\Pagination\\PaginationServiceProvider"
            ],
            "aliases": {
            }
        }
    }
}
